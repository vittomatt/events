{
    "singleQuote": true,
    "semi": true,
    "printWidth": 120,
    "tabWidth": 4,
    "trailingComma": "es5",
    "plugins": ["prettier-plugin-sort-imports"],
    "importOrder": [
      "^react$",
      "<THIRD_PARTY_MODULES>",
      "^[./]"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true
  }